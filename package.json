    "hyperscript": "^1.4.7",
    "minimist": "^1.1.1",
    "mkdirp": "^0.5.1",
    "remark": "^3.2.2",
    "remark-autolink-headings": "^2.0.0",
    "remark-slug": "^4.0.0"
  },
  "devDependencies": {
    "gh-pages": "git://github.com/markdalgleish/gh-pages#cli-message",
    "standard": "^5.4.1"
  }
}
